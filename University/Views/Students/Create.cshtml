@{
  Layout = "_Layout";
}

@model University.Models.Student 

<h3>Creat student profile</h3>

@using (Html.BeginForm())
{
  @Html.LabelFor(model => model.Name)
  @Html.TextBoxFor(model => model.Name)
  @Html.LabelFor(model => model.EnrollmentDate)
  @Html.TextBoxFor(model => model.EnrollmentDate)


  <input type="submit" value="Create profile" class="btn btn-primar" />
}

<p>@Html.ActionLink("Back to students","Index")</p>